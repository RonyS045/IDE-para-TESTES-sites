html {
    scroll-behavior: smooth;
}

body {
    font-family: 'Roboto', sans-serif;
    background: url('assets/imagens/3649ec6929c695e4323dcd94089209f8pirata.=
jpg') no-repeat center center fixed;
    background-size: cover;
    animation: backgroundAnimation 10s infinite alternate;
    margin: 0;
    padding: 0;
}

@keyframes backgroundAnimation {
    0% { filter: brightness(1); }
    100% { filter: brightness(0.8); }
}

header {
    background: rgba(0, 0, 0, 0.8);
    padding: 10px 0;
}

.menuPrincipal {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 20px;
}

.menuPrincipal .logo img {
    width: 100px;
    height: auto;
    transition: transform 0.3s, opacity 0.3s;
}

.menuPrincipal .logo img:hover {
    transform: rotate(360deg);
    opacity: 0.8;
}

.listaCategorias {
    list-style: none;
    display: flex;
    justify-content: space-around;
    flex-grow: 1;
    margin-left: 20px;
}

.listaCategorias li {
    margin: 0 10px;
}

.listaCategorias li a {
    color: white;
    text-decoration: none;
    padding: 10px 20px;
    font-weight: bold;
    transition: background 0.3s, color 0.3s;
    border-radius: 10px;
    display: flex;
    align-items: center;
}

.listaCategorias li a i {
    margin-right: 10px;
    transition: transform 0.3s;
}

.listaCategorias li a:hover {
    background: white;
    color: black;
}

.listaCategorias li a:hover i {
    transform: rotate(360deg);
}

section, article {
    padding: 50px;
    text-align: center;
}

.gallery {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    background-image: url(assets/imagens/124594.jpg);
    border-radius: 10px;
}

.gallery img {
    margin: 10px;
    width: 200px;
    height: 150px;
    object-fit: cover;
    transition: transform 0.3s, box-shadow 0.3s;
    border-radius: 5px;
}

.gallery img:hover {
    transform: scale(1.1);
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);
}

form {
    display: flex;
    flex-direction: column;
    align-items: center;
    opacity: 0;
    transform: translateY(50px);
    transition: opacity 1s ease-out, transform 1s ease-out;
    background: rgba(255, 255, 255, 0.9);
    padding: 20px;
    border-radius: 10px;
}

form.visible {
    opacity: 1;
    transform: translateY(0);
}

form label {
    margin-top: 10px;
}

form input, form textarea {
    width: 40%;
    padding: 10px;
    margin-top: 5px;
    border: 1px solid #ccc;
    border-radius: 5px;
    transition: background 0.3s, border 0.3s;
}

form input:focus, form textarea:focus {
    background: #f0f0f0;
    border: 1px solid #333;
}

form button {
    margin-top: 20px;
    padding: 10px 20px;
    background: #333;
    color: white;
    border: none;
    border-radius: 5px;
    cursor: pointer;
    transition: background 0.3s;
}

form button:hover {
    background: #555;
}

footer {
    background: rgba(0, 0, 0, 0.8);
    color: white;
    text-align: center;
    padding: 10px 0;
    position: fixed;
    width: 100%;
    bottom: 0;
}

.social-icons {
    display: flex;
    justify-content: center;
    margin-top: 10px;
}

.social-icons a {
    color: white;
    margin: 0 10px;
    font-size: 1.5em;
    transition: color 0.3s;
}

.social-icons a:hover {
    color: #00acee;
}

@media (max-width: 768px) {
    .menuPrincipal .logo img {
        width: 80px;
    }

    .listaCategorias {
        flex-direction: column;
        align-items: center;
    }

    .listaCategorias li {
        margin: 10px 0;
    }

    section, article {
        padding: 20px;
    }

    .gallery img {
        width: 150px;
        height: 100px;
    }

    form input, form textarea {
        width: 90%;
    }
}

